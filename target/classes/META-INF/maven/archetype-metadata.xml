<?xml version="1.0" encoding="UTF-8"?><archetype-descriptor name="web-archetype">
    <requiredProperties>
        <requiredProperty key="groupId">
            <defaultValue>au.com.adcowebsolutions</defaultValue>
        </requiredProperty>
        <requiredProperty key="artifactId"/>
        <requiredProperty key="title"/>
        <requiredProperty key="basedir"/>
        <requiredProperty key="url"/>
        <requiredProperty key="dbPassword"/>
    </requiredProperties>
    
    <fileSets>
        
        <fileSet filtered="true" encoding="UTF-8">
            <directory>dist</directory>
            <includes>
                <include>**/*.properties</include>
				<include>**/*.txt</include>
            </includes>
        </fileSet>
        
        <fileSet filtered="true" packaged="true" encoding="UTF-8">
            <directory>src/main/java</directory>
            <includes>
                <include>**/*.java</include>
            </includes>
        </fileSet>
        
        <fileSet filtered="true" encoding="UTF-8">
            <directory>src/main/webapp</directory>
            <includes>
                <include>**/*.html</include>
                <include>**/*.xhtml</include>
                <include>**/*.xml</include>
                <include>**/*.jsp</include>
            </includes>
        </fileSet>
        
        <fileSet encoding="UTF-8">
            <directory>src/main/webapp</directory>
            <includes>
                <include>**/*.tld</include>
                <include>**/*.gif</include>
                <include>**/*.css</include>
                <include>**/*.png</include>
                <include>**/*.jpg</include>
                <include>**/*.js</include>
            </includes>
        </fileSet>
        
        <fileSet filtered="true" packaged="true" encoding="UTF-8">
            <directory>src/test/java</directory>
            <includes>
                <include>**/*.java</include>
            </includes>
        </fileSet>
        
        <fileSet filtered="true" encoding="UTF-8">
            <directory>src/main/resources</directory>
            <includes>
                <include>**/*.properties</include>
                <include>**/*.xml</include>
                <include>**/*.sql</include>
                <include>**/*.dtd</include>
            </includes>
        </fileSet>
        
        <fileSet filtered="true" encoding="UTF-8">
            <directory></directory>
            <includes>
                <include>**/*.sh</include>
            </includes>
        </fileSet>
        
        <fileSet filtered="true" encoding="UTF-8">
            <directory>src/deploy</directory>
            <includes>
                <include>**/*.xml</include>
            </includes>
        </fileSet>
        
    </fileSets>
    
</archetype-descriptor>